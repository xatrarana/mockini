{
  "port": 3000,
  "routes": [
    {
      "method": "POST",
      "path": "/auth/login",
      "response": {
        "token": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...",
        "user": {
          "id": 1,
          "email": "user@example.com",
          "name": "John Doe"
        }
